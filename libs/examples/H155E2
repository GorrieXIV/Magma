P := Graph< 10 | [ { 2, 5, 6 }, { 1, 3, 7 }, { 2, 4, 8 }, { 3, 5, 9 },
              { 1, 4, 10 }, { 1, 8, 9 }, { 2, 9, 10 }, { 3, 6, 10 },
              { 4, 6, 7 }, { 5, 7, 8 } ] >;
PS := Graph< 10 | [ { 2, 5, 6 }, { 1, 3, 7 }, { 2, 4, 8 }, { 3, 5, 9 },
              { 1, 4, 10 }, { 1, 8, 9 }, { 2, 9, 10 }, { 3, 6, 10 },
              { 4, 6, 7 }, { 5, 7, 8 } ] : SparseRep := true >;
assert PS eq P;
PS := Graph< 10 | P : SparseRep := true >;
assert PS eq P;
