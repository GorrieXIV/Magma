print "Example: H113E2";
ei := GetEchoInput();
SetEchoInput(true);
Q := RationalField();
R<x,y,z> := LocalPolynomialRing(Q, 3);
I := Ideal([x^5 - x*y^6 + z^7, x*y + y^3 + z^3, x^2 + y^2 - z^2]);
I;
StandardBasis(I);                              
[Factorization(f): f in $1];
