// original file: Text/Algebra/AlgFP.text, line: 607
// Example: H87E2 ()
print "Example: H87E2";
ei := GetEchoInput();
SetEchoInput(true);
K := RationalField();
F<x,y,z> := FreeAlgebra(K, 3);
f := (3*x*y - 2*y*z)*(4*x - 7*z*y) + 23*x*y*z;
f;
TotalDegree(f);
assert $1 eq 4;
Coefficients(f);
Monomials(f);
Terms(f);
MonomialCoefficient(f, x*y*z);
assert $1 eq 23;
LeadingTerm(f);
LeadingCoefficient(f);
assert $1 eq -21;
m := Monomials(f)[1];
m;
Length(m);
assert $1 eq 4;
m[1];
m[2];
SetEchoInput(ei);
