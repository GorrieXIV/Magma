// original file: Text/Algebra/AlgLie.text, line: 4009
// Example: H106E44 ()
print "Example: H106E44";
ei := GetEchoInput();
SetEchoInput(true);
L:=LieAlgebra("D3",RationalField());
L;
K:=sub< L | [L.1,L.2,L.3] >;
M:=Centralizer(L, K);
M;
R:=SolvableRadical(M);
R;
HasLeviSubalgebra(M);
K:=Centralizer(L, sub< L | [L.1,L.2,L.3] >);
K;
IsSolvable(K);
assert $1;
IsNilpotent(K);
assert $1;
R:= SolvableRadical(K);
IsSolvable(R);
assert $1;
IsNilpotent(R);
assert $1;
N:= Nilradical(K);
IsNilpotent(N);
assert $1;
SetEchoInput(ei);
