// Example: H24E1 ()
print "Example: H24E1";
ei := GetEchoInput();
SetEchoInput(true);
Z := IntegerRing();
S := PolynomialRing(Z, 2);
x := S.1;
y := S.2;
f := x^3*y +3*y^2;
f;
AssignNames(~S, ["x", "y"]);
x := S.1; y := S.2;
f := x^3*y +3*y^2;
f;
S<x, y> := PolynomialRing(Z, 2);
f := x^3*y +3*y^2;
f;
SetEchoInput(ei);
