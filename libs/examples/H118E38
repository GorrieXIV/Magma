// original file: Text/Geometry/Scheme.text, line: 5236
// Example: H118E38 ()
print "Example: H118E38";
ei := GetEchoInput();
SetEchoInput(true);
P2<x,y,z>:=ProjectiveSpace(Rationals(),2);
C:=Curve(P2,x^3+y^3-2*z^3);
E,phicomp:=EllipticCurve(C,Place(C![1,1,1]));
Puvw<u,v,w>:=Ambient(E);
phi:=Expand(phicomp);
phi;
Degree(BaseScheme(phi));
assert $1 eq 6;
phiext:=Extend(phi);
phiext;
Degree(BaseScheme(phiext));
assert $1 eq 0;
bl,phii:=IsInvertible(phiext);
assert bl;
phii;
Degree(BaseScheme(phii));
assert $1 eq 6;
phiiext:=Extend(phii);
Degree(BaseScheme(phiiext));
assert $1 eq 0;
phiiext;
SetEchoInput(ei);
