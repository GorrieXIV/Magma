print "Example: H88E9";
ei := GetEchoInput();
SetEchoInput(true);
M := MatrixAlgebra(Rationals(),2);
x := M![0,1,-2,0];
y := M![0,3,-6,0];
CommonEigenspaces([x,y]);
Diagonalisation(sub<M|x,y>);                              
SetEchoInput(ei);
