print "Example: H9E8";
ei := GetEchoInput();
SetEchoInput(true);
p := 10007;
F := FiniteField(p);
proots := {  z : z in F | IsPrimitive(z) };
#proots;
Random(proots);
random{ x : x in F | IsPrimitive(x) };
SetEchoInput(ei);
