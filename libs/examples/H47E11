// Example: H47E11 ()
print "Example: H47E11";
ei := GetEchoInput();
SetEchoInput(true);
R := pAdicRing(2);
pi := UniformizingElement(R);
pi;
1 / pi;
Parent($1);
IsExactlyDivisible(1, pi);
assert not $1;
IsExactlyDivisible(pi^2, pi);
SetEchoInput(ei);
