// Example: H88E12 ()
print "Example: H88E12";
ei := GetEchoInput();
SetEchoInput(true);
G := Sym(5);
H := Normalizer(G,Sylow(G,3));
M := PermutationModule(G,H, GF(2));
M;
A := Action(M);
P, I, mu  := Presentation(A);
Dimension(P/I);
assert $1 eq 42;
CartanMatrix(A);
B := CondensedAlgebra(A);
Q, J, theta := Presentation(B);
J;
boo, y := WordProblem(A,A.1);
boo;
assert $1;
y;
mu(y); 
mu(y) eq A.1;
assert $1;
b := Random(Generic(A));
WordProblem(A,b);
assert not $1;
SetEchoInput(ei);
