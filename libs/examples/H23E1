ei := GetEchoInput();
SetEchoInput(true);
Q := RationalField();
P<x> := PolynomialRing(Q);
PP := PolynomialRing(Q);    
P;
PP;
PP.1;
Pa<a> := PolynomialRing(Q: Global := false);
