// Example: H44E37 ()
print "Example: H44E37";
ei := GetEchoInput();
SetEchoInput(true);
Qc<c> := PolynomialRing(RationalField());
Qcs<s> := PolynomialRing(Qc);
F<s> := FunctionField(s^2 + c^2 - 1);
c := F!c;
Genus(F);
assert $1 eq 0;
Zeros(s);
Zeros(c-1);
P := Zeros(c-1)[1];
Degree(P);
assert $1 eq 1;
Dimension(1*P);
assert $1 eq 2;
Basis(1*P);
a := Basis(1*P)[1];
Degree(a);
assert $1 eq 1;
MinimalPolynomial(a);
(a^2 - 1)/(a^2 + 1);
a * ((a^2 - 1)/(a^2 + 1) - 1);
SetEchoInput(ei);
