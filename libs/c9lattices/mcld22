//Standard generators of McL are a, b where a is in class 2A,
//b is in class 5A, ab has order 11 and ababababbababbabb has order 7.
_LR := rec < recformat< F: GrpFP, AI: SeqEnum, G: GrpMat > |
      F := FreeGroup(2) >;
_LR`AI := [ [a,
      b^-1*a*b^-2*a*b^-1*a*b*a*b^2*a*b^-2*a*b*a*b^-1*a*b^-2*a*b^-1*a] ]
where a is (_LR`F).1 where b is (_LR`F).2;
//one consitutent

