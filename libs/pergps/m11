
print "M11 - Mathieu group on 11 letters - degree 11 ";
print "Order 7 920 = 2^4 * 3^2 * 5 * 11;  Base 1,2,3,4 ";
print "Group: G";

G := PermutationGroup<11 |
    \[10,  8, 11,  4,  7,  6,  5,  2,  9,  1,  3],
    \[ 4, 11,  3,  7,  5,  1,  6,  8,  2,  9, 10]>;
AssertAttribute(G, "Order", 2^4 * 3^2 * 5 * 11);
