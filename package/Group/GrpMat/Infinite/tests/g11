G := MatrixGroup<7, K | [
    Matrix(SparseMatrix(K, 7, 7, [
    <1, 1, 1>, <2, 1, w>, <2, 2, 1>, <3, 3, 1>, <4, 4, 1>, <5, 5, 1>, <6, 1, 
    2*w>, <6, 6, 1>, <7, 7, 1>])),
    DiagonalMatrix(K, [ w, 2, 1, 1, 1, 1, 1 ]),
    Matrix(SparseMatrix(K, 7, 7, [
    <1, 1, 1>, <2, 2, 1>, <3, 3, 1>, <4, 4, 1>, <5, 3, -163208757248*w>, <5, 5, 
    1>, <6, 6, 1>, <7, 7, 1>])),
    Matrix(SparseMatrix(K, 7, 7, [
    <1, 1, 1>, <2, 2, 1>, <3, 3, 1>, <4, 2, 25769803767*w>, <4, 4, 1>, <5, 5, 
    1>, <6, 6, 1>, <7, 7, 1>]))
]> where w := K.1 where K := ext<K|Polynomial(K, [-2, 0, 1])> where K is 
RationalField()
;

IsSolubleByFinite (G);
HirschNumber (G);
PrueferRankBound (G);

