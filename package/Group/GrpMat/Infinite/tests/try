SetSeed (1);
SetEchoInput (true);
load "../ut.m";
Q := Rationals ();
DB := RationalMatrixGroupDatabase ();
d:=4;e:=2; 
X := Group(DB, 4, 2);
Y := Group(DB, 2, 1);
D:=[DiagonalJoin (X.i, Y.i): i in [1..Ngens (Y)]];
G:=sub<GL(d + e, Q) | D >;
X := UT (6, Q);
d:=DiagonalMatrix (Q, [3, 2, -1, 4,5,-2]);
U := sub<GL(6, Q) | d, X>;
IsNilpotent (U);
H:=[KroneckerProduct (G.i, U.j): i in [1..Ngens (G)], j in [1..4]];
H:=sub<GL(36, Q) | H>;
Ngens (H);
IsSolubleByFinite (H);
IsNilpotentByFinite (H);
time HirschNumber (H);
Ngens (H);
K:=sub< H | [H.Random ([1..8]) * H.Random ([1..8]): i in [1..4]]>;

