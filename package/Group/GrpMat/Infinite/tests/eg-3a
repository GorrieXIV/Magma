load "../ut.m";
SetEchoInput (true);
Q := Rationals ();
DB := RationalMatrixGroupDatabase ();
d:=4;e:=2; 
X := Group(DB, 4, 2);
Y := Group(DB, 2, 1);
D:=[DiagonalJoin (X.i, Y.i): i in [1..2], j in [1..1]];
G:=sub<GL(d + e, Q) | D >;
X := UT (6, Q);
d:=DiagonalMatrix (Q, [3, 2, -1, 4,5,-2]);
U := sub<GL(6, Q) | d, X[1]>;
IsNilpotent (U);
H:=[KroneckerProduct (G.i, U.j): i in [1..Ngens (G)], j in [1..Ngens (U)]];
H := [Random (H): i in [1..4]];
H:=sub<GL(36, Q) | H>;
Ngens (H);
IsSolubleByFinite (H);
IsNilpotentByFinite (H);
time HirschNumber (H);
